{{#ermes-panel id=panelId title=(t 'fields.text.crop-phenology')}}
  <form>
    <label>
        <span>{{t "panel.crop-phenology.date"}}</span>
      {{jqm-datepicker}}
    </label>
    <label>
        <span>{{t "panel.crop-phenology.development-stage"}}</span>
      {{jqm-select theme="d" options=developmentStages}}
    </label>
    <label>
        <span>{{t "panel.crop-phenology.bbch"}}</span>
      {{jqm-select theme="d" options=phenologyGrowth}}
    </label>
    {{jqm-select theme="d" options=actualPhenologyCodes}}
    {{jqm-button text=(t "fields.text.add-new") action=(action "submit" on="submit") theme="b"}}
  </form>
{{/ermes-panel}}